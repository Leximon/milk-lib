{
  "schemaVersion": 1,
  "id": "milk",
  "version": "${version}",
  "name": "Milk Lib",
  "description": "Literally just adds milk, to act as a bridge for any other mods that want to do the same.",
  "authors": [
    "Tropheus Jay"
  ],
  "contact": {
    "sources": "https://github.com/TropheusJ/milk-lib"
  },
  "license": "MIT",
  "icon": "assets/milk/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.tropheusj.milk.Client"
    ]
  },
  "mixins": [
    "milk.mixins.json"
  ],
  "accessWidener": "milk.accesswidener",
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": ">=0.47.8+1.18.2",
    "minecraft": ">=1.18.1",
    "java": ">=17"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    },
    "fluidlogged": "fluidlogged.mod.json"
  }
}
